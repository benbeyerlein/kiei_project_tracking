<div class="page-header">
  <h1>Tracking number statuses</h1>
</div>

<p><a href="/tracking_number_statuses/new" class="btn btn-primary">Add a New Tracking Number Status</a></p>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Status</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @tracking_number_statuses.each do |tracking_number_status| %>
      <tr>
        <td><%= tracking_number_status.status %></td>
        <td>
          <a href="/tracking_number_statuses/<%= tracking_number_status.id %>" class="btn btn-primary">
            <i class="fa fa-search-plus"></i>
          </a>
        </td>
        <td>
          <a href="/tracking_number_statuses/<%= tracking_number_status.id %>/edit" class="btn btn-warning">
            <i class="fa fa-edit"></i>
          </a>
        </td>
        <td>
          <a href="/delete_tracking_number_status/<%= tracking_number_status.id %>" class="btn btn-danger">
            <i class="fa fa-trash-o"></i>
          </a>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

